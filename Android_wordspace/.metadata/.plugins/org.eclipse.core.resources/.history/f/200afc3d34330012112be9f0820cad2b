	private int CategoryID;
	private String CategoryName;	
	private int ProductID;
	private String ProductName;
	
	//select Items.IID,ProductID,BrandID,Name
	private int ItemID;
    private String ItemName;
  
    private int BrandID;
    private String BrandName;
    
    
	 
 
}